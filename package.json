{
  "name": "bodega-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot for Bodega Cats GC with Activity App integration",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"public/**/*.{html,css,js}\"",
    "register": "ts-node scripts/deploy-commands.ts",
    "test": "jest",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/bodega-discord-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "bodega-cats",
    "activity-app"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/bodega-discord-bot/issues"
  },
  "homepage": "https://github.com/yourusername/bodega-discord-bot#readme",
  "dependencies": {
    "@discord/embedded-app-sdk": "^2.0.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.19.0",
    "@supabase/supabase-js": "^2.56.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "cors": "^2.8.5",
    "cron": "^4.3.0",
    "discord.js": "^14.19.2",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^149.0.0",
    "node-fetch": "^2.7.0",
    "winston": "^3.17.0",
    "wrangler": "^4.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.100",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "cpx": "^1.5.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
